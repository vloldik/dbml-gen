Table user {
    firstname varchar(255)
    lastname varchar(255)
    name_a date
    name_b int
    id int [pk, increment]
}

Table test_user {
    id int [pk, increment]
    test varchar(124)
    user_id int [ref: - user.id]
    user_id1 int [ref: > user.id]
}

Table user_tests {
    test_id int [ref: <> test_user.id]
    user_id int [ref: <> user.id]
    testField varchar(255)

    indexes {
        (user_id, test_id) 
    }
}

Table users1 {
  id integer
  username varchar
  role varchar
  created_at timestamp
}

Table posts {
  id integer [primary key]
  title varchar
  body text [note: 'Content of the post']
  user_id integer
  status post_status
  created_at timestamp
}